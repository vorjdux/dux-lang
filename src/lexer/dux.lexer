%baseclass-header = "LexerBase.h"
%class-header = "Lexer.h"
%implementation-header = "LexerImpl.h"
%class-name = "Lexer"
%lex-source = "Lexer.cpp"

%namespace = "dux"

%debug
%no-lines

%x pre line comment string rawstring optws


%%

[ \t\n]+                            // skip white space chars.
[0-9]+                              return NUMBER;
[[:alpha:]_][[:alpha:][:digit:]_]*  return IDENTIFIER;
.                                   return matched()[0];
