%namespace dux

%baseclass-header ParserBase.h
%class-header Parser.h
%implementation-header ParserImpl.h
%parsefun-source Parser.cpp

%scanner             ../lexer/Scanner.h
%scanner-token-function d_scanner.lex()

%token IDENTIFIER NUMBER CHAR

%%

startrule:
    startrule tokenshow
|
    tokenshow
;

tokenshow:
    token
    {
        std::cout << "matched: " << d_scanner.matched() << '\n';
    }
;

token:
    IDENTIFIER
|
    NUMBER
|
    CHAR
;
